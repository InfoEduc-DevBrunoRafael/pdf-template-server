<% 
isFalsy = function(value) {
    if (value === "null" || value === null) {
        return true
    }
    return false
}

string = function(str) {
    if (isFalsy(str)) return ""
    return str
}

number = function(num) {
    if (isNaN(num)) return ""
    return num.toString()
}

padWithZero = function(num) {
    return num.toString().padStart(2, "0")
}

 %> 