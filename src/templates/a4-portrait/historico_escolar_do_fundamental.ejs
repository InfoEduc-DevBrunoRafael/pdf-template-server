            transform: rotate(270deg);
            position: relative;
            white-space: nowrap;
        }
        .text{
            position: relative;
            left: -70px;
        }
        .text2{
            position: relative;
            left: -60px;
        }
        .text3{
            position: relative;
            left: -30px;
        }
        .text4{
            position: relative;
            left: -280px;
            font-size: 14px;
        }
        .parte_diversificada{
            height: 170px;
            transform: rotate(270deg);
            position: relative;
            white-space: nowrap;
        }
        .tex-vertical{
            max-width: 20px;
            height: 150px;
            transform: rotate(270deg);
            position: relative;
            white-space: nowrap;
        }
        .tex-vertical2{
            max-width: 20px;
            height: 300px;
            transform: rotate(270deg);
            position: relative;
            white-space: nowrap;
        }
        .center{
            text-align: center;

        }
        .celulaTitle{
            text-align: left;
            font-weight: bold;
            font-size: 14px;
            text-transform: uppercase;
        }
        .page-break {
            break-before: page;
        }
    </style>
