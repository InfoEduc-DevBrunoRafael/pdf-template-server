                    <div class="multicoluna">
                        <div class="inline">
                            <b>Nome do Aluno (a):</b>
                            <div class="texto-linha"><%= cabecalho_aluno.nome %> <%= cabecalho_aluno.sobrenome %></div>
                        </div>
                    </div>
                    <div class="multicoluna">
                        <div class="inline">
                            <b>ID do INEP:</b>
                            <div class="texto-linha"><%= cabecalho_aluno.id_inep %> </div>
                        </div>
